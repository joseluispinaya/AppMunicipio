<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppMunicipio.Mobile.Views.Dashboard.InicioView"
             Title="InicioView">
    <ScrollView>
        <VerticalStackLayout
        Spacing="5">
            <Frame
                Margin="5"
                Padding="5">
                <Image Source="logoemi.png"/>
            </Frame>
            <Image Source="logojo.png" HeightRequest="150" WidthRequest="150" Margin="0, -50,0,0" />
            <Label
                Text="Sistema de Control"
                TextColor="Black"
                FontSize="Title"
                FontAttributes="Bold"
                HorizontalTextAlignment="Center"/>

        </VerticalStackLayout>
    </ScrollView>
</ContentPage>